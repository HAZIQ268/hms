{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "$": "readonly",
    "jQuery": "readonly"
  },
  "ignorePatterns": [
    "public/admin/vendor/**/*.js"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-undef": "off"
  }
}
